# Backend Environment Variables
# Copy this file to .env and fill in your values

# Server Port (Render will set this automatically)
PORT=3001

# Node Environment
NODE_ENV=production

# Allowed CORS Origins
# For production, add your Vercel frontend URL
# Multiple origins can be separated by commas
# Example: https://heightech-x.vercel.app,https://www.yourdomain.com
ALLOWED_ORIGINS=https://heightech-x.vercel.app

# Supabase Configuration (if needed for backend)
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

